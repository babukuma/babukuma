<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>babukuma-demo</application>
	<version>2</version>
	
	<static-files>
		<include path="**.html" />
		<include path="**.js" />
		<include path="**.png" />
		<include path="/html5/**.*" />
	</static-files>
	
	<!-- Configure java.util.logging -->
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
	</system-properties>

</appengine-web-app>